{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "systemd",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "section"
        },
        {
          "type": "SYMBOL",
          "name": "directive"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "blank_line"
        }
      ]
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "section_name"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "section_name": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "common_section"
        },
        {
          "type": "SYMBOL",
          "name": "service_section"
        },
        {
          "type": "SYMBOL",
          "name": "mount_section"
        },
        {
          "type": "SYMBOL",
          "name": "socket_section"
        },
        {
          "type": "SYMBOL",
          "name": "timer_section"
        },
        {
          "type": "SYMBOL",
          "name": "path_section"
        },
        {
          "type": "PATTERN",
          "value": "[A-Za-z0-9]+"
        }
      ]
    },
    "common_section": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "Unit"
        },
        {
          "type": "STRING",
          "value": "Install"
        }
      ]
    },
    "service_section": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "Service"
        },
        {
          "type": "STRING",
          "value": "Kill"
        },
        {
          "type": "STRING",
          "value": "Restart"
        }
      ]
    },
    "mount_section": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "Mount"
        },
        {
          "type": "STRING",
          "value": "Automount"
        }
      ]
    },
    "socket_section": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "Socket"
        },
        {
          "type": "STRING",
          "value": "SocketActivation"
        }
      ]
    },
    "timer_section": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "Timer"
        },
        {
          "type": "STRING",
          "value": "OnCalendar"
        }
      ]
    },
    "path_section": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "Path"
        },
        {
          "type": "STRING",
          "value": "PathExists"
        }
      ]
    },
    "directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "key"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "value"
        }
      ]
    },
    "key": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9_]+"
    },
    "value": {
      "type": "PATTERN",
      "value": "[^\\n#]+"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "whitespace"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]*"
        }
      ]
    },
    "blank_line": {
      "type": "PATTERN",
      "value": "[\\s\\t]+"
    },
    "whitespace": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}